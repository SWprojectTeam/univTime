package com.example.univTime.univTime.repository;

import com.example.univTime.univTime.domain.User;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;
import java.util.Optional;

public interface UserRepository<User> extends JpaRepository<User, Long> {
    <User> Optional<User> findByUsername(String username);
}
